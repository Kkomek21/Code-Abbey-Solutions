suits = ['Clubs', 'Spades', 'Diamonds', 'Hearts']
ranks = ['2', '3', '4', '5', '6', '7', '8', '9', '10', 'Jack', 'Queen', 'King', 'Ace']

def Card_Name(value):
    suit = suits[int((value/13))]
    rank = ranks[(value%13)]
    print(rank,'-of-',suit,sep='')

Card_Name(25)
Card_Name(32)
Card_Name(51)
Card_Name(20)
Card_Name(6)

#Code written in Python 3.8.3

def Array_Counters(array):
    Counters = {}
    for i in array:
        if str(i) not in Counters:   # if i isnt already in the dictionary, we give it the value of 1
            i = str(i)
            Counters[i] = 1
        else:                        # If it already is, we increment the value by 1                      
            i = str(i)
            Counters[i] = Counters[i] +1

    print(Counters)


array = [3, 2, 1, 2, 3, 1, 1, 1, 1, 3]
Array_Counters(array)
            

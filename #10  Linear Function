#Code written in Python 3.8.3



def Linear(x1,y1,x2,y2):
    a = (int(y1)-int(y2)) / (int(x1)-int(x2))      #Formula derived from  { y1 = a*x1 +b
                                                   #                      { y2 = a*x2 +b
    b = y1- a*x1                                 
    
    print(a,b)


Linear(0,0,1,1)
Linear(1,0,0,1)

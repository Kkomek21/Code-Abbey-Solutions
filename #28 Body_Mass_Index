def BMI(weight,height):
    bmi = weight/ (height**2)
    if bmi < 18.5:
        print('under')
    elif bmi >= 18.5 and bmi <25:
        print('normal')
    elif bmi >= 25 and bmi <30:
        print('over')
    elif bmi >= 30:
        print('obese')

BMI(80,1.73)
BMI(55,1.58)
BMI(49,1.91)
        
